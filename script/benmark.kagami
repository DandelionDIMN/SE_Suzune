fn BubbleSort(array_size)
  ar = array(array_size, 0)
  i = 0
  num = array_size
  
  while(i < array_size)
    ar[i] = num
    num = num - 1
    i = i + 1
  end
  
  println('Sorting.(Bubble)')
  
  startTime = time()
  i = 0
  
  while(i < array_size)
    j = i + 1
    
    while(j < array_size)
      if(ar[i] > ar[j]) 
        swap(ar[i],ar[j])
      end
      
      j = j + 1
    end
    
    i = i + 1
  end
  
  println('Start Time:' + startTime)
  println('End Time  :' + time())
end

fn InsertionSort(array_size)
  ar = array(array_size, 0)
  i = 0
  num = array_size - 1
  
  while(i < array_size)
    ar[i] = num
    num = num - 1
    i = i + 1
  end
  
  println('Sorting.(Insertion)')
  
  startTime = time()
  i = 0
  
  while(i < array_size)
    j = i + 1
    best_idx = i
    
    while(j < array_size)
      if(ar[j] < ar[best_idx])
        best_idx = j
      end
    
      j = j + 1
    end
    
    if(best_idx != i)
      swap(ar[i],ar[best_idx])
    end
    
    i = i + 1
  end
  
  println('Start Time:' + startTime)
  println('End Time  :' + time())
  
  if(array_size <= 10)
    println(ar)
  end
end

fn main()
  value = 10
  
  mode = convert(input('Which algorithm?(1:Bubble 2.Insertion) \n>'))
  func = 0
  
  case(mode)
  when(1)
    func = BubbleSort
  when(2)
    func = InsertionSort
  end
  
  while(value != 'q')
    value = convert(input('array size? (q=exit) \n>'))
    
    if(type(value) == kStringTypeInteger)
      func(value)
    end
    
    println('-----------------------\n')

  end
  
  println('exit.')
  
end

main()